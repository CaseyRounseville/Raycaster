const SIZE = 32;

const create = (north, south, west, east, floor, ceiling, arg1, arg2, arg3, arg4) => {
	return {
		north,
		south,
		west,
		east,
		floor,
		ceiling,
		arg1,
		arg2,
		arg3,
		arg4
	};
};

const pixelsToBlocks = (pixels) => {
	return pixels / SIZE;
};

export default {
	SIZE,
	
	create,
	pixelsToBlocks
};